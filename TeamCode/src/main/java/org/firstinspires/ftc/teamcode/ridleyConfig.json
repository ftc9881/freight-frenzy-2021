{
  "driveTrain": {
    "type": "TANK",
    "telemetry": true,
    "motors": {
      "left": {
        "name": "left_drive",
        "direction": "FORWARD",
        "clicksPerMeter": 1500
      },
      "right": {
        "name": "right_drive",
        "direction": "REVERSE",
        "clicksPerMeter": 1500
      }
    }
  },
  "devices": {
    "liftMotor": {
      "type": "MOTOR",
      "name": "lift_motor",
      "direction": "FORWARD",
      "telemetry": true
    },
    "rightIntakeMotor": {
      "type": "MOTOR",
      "name": "lift_motor",
      "direction": "FORWARD",
      "velocity": false,
      "maxVelocity": 36000,
      "telemetry": true,
      "speedExponent": 2,
      "speedScale": ".75"
    },
    "leftIntakeMotor": {
      "type": "MOTOR",
      "name": "lift_motor",
      "direction": "REVERSE",
      "velocity": false,
      "maxVelocity": 36000,
      "telemetry": true,
      "speedExponent": 2,
      "speedScale": ".75"
    },
    "basketServo": {
      "type": "SERVO",
      "name": "basket_servo",
      "telemetry": true
    }
  },
  "sensors": {
    "imu_control": {
      "type": "IMU",
      "name": "imu_control",
      "telemetry": true,
      "log": false,
      "axesOrder": "ZYX",
      "xAxis": "Z",
      "yAxis": "Y",
      "zAxis": "X",
      "reverse": false
    }
  },
  "controllerOne": {
    "type": "TANK_STEER",
    "telemetry": true,
    "controls": {
      "LEFT_X": {
        "telemetry": false
      },
      "LEFT_Y": {
        "telemetry": false
      },
      "RIGHT_X": {
        "telemetry": false
      },
      "RIGHT_Y": {
        "telemetry": false
      },
      "A": {
        "telemetry": true,
        "actions": [
          {
            "activity": "PRESS",
            "type": "PROPERTY",
            "deviceName": "[drive_train]",
            "property": "mode",
            "value": "micro"
          },
          {
            "activity": "RELEASE",
            "type": "PROPERTY",
            "deviceName": "[drive_train]",
            "property": "mode",
            "value": "macro"
          }
        ]
      },
      "B": {
        "telemetry": true,
        "actions": [
          {
            "activity": "PRESS",
            "type": "BEHAVIOR",
            "deviceName": "basketServo",
            "behavior": "MAXIMUM"
          },
          {
            "activity": "RELEASE",
            "type": "BEHAVIOR",
            "deviceName": "basketServo",
            "behavior": "MINIMUM"
          }
        ]
      },
      "LEFT_BUTTON": {
        "telemetry": true,
        "actions": [
          {
            "activity": "PRESS",
            "type": "BEHAVIOR",
            "deviceName": "liftMotor",
            "behavior": "REVERSE"
          },
          {
            "activity": "RELEASE",
            "type": "BEHAVIOR",
            "deviceName": "liftMotor",
            "behavior": "STOP"
          }
        ]
      },
      "RIGHT_BUTTON": {
        "telemetry": true,
        "actions": [
          {
            "activity": "PRESS",
            "type": "BEHAVIOR",
            "deviceName": "liftMotor",
            "behavior": "FORWARD"
          },
          {
            "activity": "RELEASE",
            "type": "BEHAVIOR",
            "deviceName": "liftMotor",
            "behavior": "STOP"
          }
        ]
      },
      "LEFT_TRIGGER": {
        "telemetry": true,
        "actions": [
          {
            "activity": "CHANGE",
            "type": "BEHAVIOR",
            "deviceName": "leftIntakeMotor",
            "behavior": "FORWARD"
          },
          {
            "activity": "CHANGE",
            "type": "BEHAVIOR",
            "deviceName": "rightIntakeMotor",
            "behavior": "FORWARD"
          }
        ]
      },
      "RIGHT_TRIGGER": {
        "telemetry": true,
        "actions": [
          {
            "activity": "CHANGE",
            "type": "BEHAVIOR",
            "deviceName": "leftIntakeMotor",
            "behavior": "REVERSE"
          },
          {
            "activity": "CHANGE",
            "type": "BEHAVIOR",
            "deviceName": "rightIntakeMotor",
            "behavior": "REVERSE"
          }
        ]
      }
    }
  }
}
